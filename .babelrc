  "plugins": [["import", { "libraryName": "antd", "style": "css" }]],
  "env": {
    "production": { "plugins": ["emotion"] },
    "development": { "plugins": [["emotion", { "sourceMap": true }]] },
    "test": { "plugins": [["emotion", { "sourceMap": true }]] }
  }
}
